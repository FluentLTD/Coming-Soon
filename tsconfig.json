{
    "include": [
        ".astro/types.d.ts",
        "**/*"
    ],
    "exclude": [
        "dist"
    ],
    "compilerOptions": {
        "jsx": "react-jsx",
        "jsxImportSource": "react",
        "module": "ESNext",
        "moduleResolution": "bundler",
        "target": "ES2022",
        "resolveJsonModule": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "noEmit": true,
        "strict": true,
        "allowJs": true
    }
}